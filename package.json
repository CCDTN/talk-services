{
  "name": "talk-services",
  "version": "2.2.2",
  "description": "talk-services",
  "main": "./lib/service.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "[ -f test/config.json ] || cp test/config.example.json test/config.json && NODE_ENV=test ./node_modules/.bin/mocha --require coffee-script/register --require should --reporter spec test/main.coffee",
    "prepublish": "rm -rf lib && coffee -o lib -c src",
    "deploy": "npm run uploadcdn && npm publish",
    "doc": "cd docs && gitbook build",
    "uploadcdn": "osscmd uploadfromdir images oss://dn-talk/images"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:teambition/talk-services.git"
  },
  "author": {
    "name": "Xu Jingxin",
    "email": "sailxjx@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "limbo": "^0.4.0",
    "mocha": "^2.3.4",
    "mongoose": "^4.2.7",
    "should": "^7.1.1"
  },
  "dependencies": {
    "bluebird": "^3.0.5",
    "body-parser": "^1.14.1",
    "charset": "^1.0.0",
    "coffee-script": "^1.10.0",
    "config": "^1.17.1",
    "err1st": "^0.2.4",
    "express": "^4.13.3",
    "feedparser": "^1.1.4",
    "glob": "^6.0.1",
    "graceful-logger": "^0.4.3",
    "he": "^0.5.0",
    "hiredis": "^0.4.1",
    "iconv-lite": "^0.4.13",
    "jschardet": "^1.4.1",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "morgan": "^1.6.1",
    "qs": "^6.0.0",
    "redis": "^2.3.1",
    "request": "^2.67.0",
    "require-dir": "^0.3.0",
    "sundae": "^0.5.10",
    "validator": "^4.3.0"
  }
}
